class Solution:
    def intersection(self, nums1: List[int], nums2: List[int]) -> List[int]:
        '''
        #暴力遍历
        reList = []
        for i in set(nums1):
            for j in set(nums2):
                if i == j:
                    reList.append(i)
        return reList
        '''   
        return list(set(nums1)&set(nums2)) #python 集合操作
